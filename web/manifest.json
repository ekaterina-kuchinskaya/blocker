{
    "manifest_version": 2,

    "name": "URL_Block",
    "description": "This extension blocks suspicious pages.",
    "version": "1.0",

    "background" :
    {
        "scripts" : ["script.js"]
    },
    "options_page":
        "options.html",
    "browser_action":
    {
        "default_icon": "URL.png",
        "default_popup": "options.html"
    },
    "permissions":
        [
            "tabs"
        ]
}
